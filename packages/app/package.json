{
  "name": "biliLive-tools",
  "version": "1.6.0",
  "description": "B站直播录制处理工具",
  "main": "./out/main/index.js",
  "author": "renmu123",
  "license": "GPL-3.0",
  "homepage": "https://github.com/renmu123/biliLive-tools",
  "type": "module",
  "scripts": {
    "dev": "chcp 65001 && electron-vite dev",
    "devMain": "chcp 65001 && electron-vite dev --watch",
    "start": "electron-vite preview",
    "build": "pnpm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:app": "pnpm run build && electron-builder --config",
    "build:app:no-ffmpeg": "pnpm run build && electron-builder --config electron-builder-no-ffmpeg.yml",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "pnpm run typecheck:node && pnpm run typecheck:web",
    "test": "vitest"
  },
  "dependencies": {
    "@biliLive-tools/http": "workspace:*",
    "@biliLive-tools/shared": "workspace:*",
    "@biliLive-tools/types": "workspace:*",
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@napi-rs/canvas": "^0.1.55",
    "assjs": "^0.1.2",
    "axios": "^1.7.7",
    "better-sqlite3": "11.2.1",
    "driver.js": "^1.3.1",
    "electron-log": "^5.2.0",
    "electron-store": "^10.0.0",
    "electron-updater": "^6.3.4",
    "semver": "^7.6.3",
    "electron-context-menu": "4.0.4"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.2",
    "@electron-toolkit/eslint-config-ts": "^2.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/rebuild": "^3.6.0",
    "@rushstack/eslint-patch": "^1.10.4",
    "@types/better-sqlite3": "^7.6.11",
    "@vicons/fluent": "^0.12.0",
    "@vicons/ionicons5": "^0.12.0",
    "@vicons/material": "^0.12.0",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vueuse/components": "^11.0.3",
    "@vueuse/core": "^11.0.3",
    "artplayer": "^5.1.7",
    "electron": "32.0.2",
    "electron-builder": "^24.13.3",
    "electron-devtools-installer": "^3.2.0",
    "electron-vite": "^2.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.28.0",
    "filenamify": "^6.0.0",
    "flv.js": "^1.6.2",
    "hotkeys-js": "^3.13.7",
    "json5": "^2.2.3",
    "less": "^4.2.0",
    "marked": "^14.1.1",
    "naive-ui": "^2.39.0",
    "pinia": "^2.2.2",
    "sortablejs": "^1.15.3",
    "unplugin-auto-import": "^0.18.2",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.3",
    "vue": "^3.5.3",
    "vue-router": "^4.4.3",
    "vue-tsc": "^2.1.6",
    "vuedraggable": "^4.1.0"
  }
}
