{
  "name": "biliLive-tools",
  "version": "1.3.0",
  "description": "B站直播录制处理工具",
  "main": "./out/main/index.js",
  "author": "renmu123",
  "license": "GPL-3.0",
  "homepage": "https://github.com/renmu123/biliLive-tools",
  "type": "module",
  "scripts": {
    "dev": "chcp 65001 && electron-vite dev",
    "devMain": "chcp 65001 && electron-vite dev --watch",
    "start": "electron-vite preview",
    "build": "pnpm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:app": "pnpm run build && electron-builder --config",
    "build:app:no-ffmpeg": "pnpm run build && electron-builder --config electron-builder-no-ffmpeg.yml",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "pnpm run typecheck:node && pnpm run typecheck:web",
    "test": "vitest"
  },
  "dependencies": {
    "@biliLive-tools/http": "workspace:*",
    "@biliLive-tools/shared": "workspace:*",
    "@biliLive-tools/types": "workspace:*",
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@napi-rs/canvas": "^0.1.53",
    "@renmu/bili-api": "^1.3.4",
    "assjs": "^0.1.0",
    "axios": "^1.7.2",
    "electron-log": "^5.1.5",
    "electron-store": "^10.0.0",
    "electron-updater": "^6.2.1",
    "lodash-es": "^4.17.21",
    "ntsuspend": "^1.0.2",
    "semver": "^7.6.2",
    "subtitle": "^4.2.1"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.2",
    "@electron-toolkit/eslint-config-ts": "^2.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@rushstack/eslint-patch": "^1.10.3",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/node": "20.12.2",
    "@vicons/ionicons5": "^0.12.0",
    "@vicons/material": "^0.12.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vueuse/core": "^10.11.0",
    "electron": "31.0.2",
    "electron-builder": "^24.13.3",
    "electron-devtools-installer": "^3.2.0",
    "electron-vite": "^2.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.26.0",
    "less": "^4.2.0",
    "naive-ui": "^2.38.2",
    "qrcode-vue3": "^1.6.8",
    "unplugin-auto-import": "^0.17.6",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.3.2",
    "vue": "^3.4.31",
    "vue-router": "^4.4.0",
    "vuedraggable": "^4.1.0",
    "sortablejs": "^1.15.2",
    "flv.js": "^1.6.2",
    "artplayer": "^5.1.6",
    "@vueuse/components": "^10.11.0",
    "filenamify": "^6.0.0",
    "marked": "^13.0.1",
    "pinia": "^2.1.7",
    "vue-tsc": "^2.0.22"
  }
}
