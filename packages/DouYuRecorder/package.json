      "development": "./src/*.ts",
      "default": "./lib/*.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "douyu",
    "recorder",
    "bililive-tools",
    "直播",
    "录制",
    "斗鱼"
  ],
  "repository": "https://github.com/renmu123/biliLive-tools/tree/master/packages/DouYuRecorder",
  "author": "renmu123",
  "license": "LGPL",
  "dependencies": {
    "@bililive-tools/manager": "workspace:*",
    "mitt": "^3.0.1",
    "query-string": "^9.1.1",
    "safe-eval": "^0.4.1",
    "ws": "^8.18.0",
    "lodash-es": "catalog:",
    "axios": "catalog:",
    "douyu-api": "^0.1.0"
  },
  "devDependencies": {
    "@types/ws": "^8.5.13"
  },
  "peerDependencies": {
    "@bililive-tools/manager": "*"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.8",
    "utf-8-validate": "^6.0.5"
  }
