{
  "name": "@autorecord/douyu-recorder",
  "version": "1.2.2",
  "description": "LAR douyu recorder implemention",
  "main": "./lib/index.js",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "development": "./src/index.ts",
      "default": "./lib/index.js"
    },
    "./*.js": {
      "types": "./src/*.ts",
      "development": "./src/*.ts",
      "default": "./lib/*.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w"
  },
  "files": [
    "lib"
  ],
  "repository": "https://github.com/WhiteMinds/DouYuRecorder",
  "author": "WhiteMind",
  "license": "LGPL",
  "dependencies": {
    "@autorecord/manager": "workspace:*",
    "axios": "1.7.4",
    "cheerio": "^1.0.0",
    "crypto-js": "^4.2.0",
    "mitt": "^3.0.1",
    "query-string": "^9.1.0",
    "uuid": "^8.3.2",
    "vm2": "^3.9.10",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@biliLive-tools/types": "workspace:*",
    "@types/crypto-js": "^4.2.0",
    "@types/uuid": "^8.3.4",
    "@types/ws": "^8.5.3"
  },
  "peerDependencies": {
    "@autorecord/manager": "*"
  }
}
