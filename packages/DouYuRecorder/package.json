{
  "name": "@bililive-tools/douyu-recorder",
  "version": "1.0.2",
  "description": "bililive-tools douyu recorder implemention",
  "main": "./lib/index.js",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "development": "./src/index.ts",
      "default": "./lib/index.js"
    },
    "./*.js": {
