{
  "name": "@autorecord/bilibili-recorder",
  "version": "1.2.0",
  "description": "LAR bilibili recorder implemention",
  "main": "./lib/index.js",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "development": "./src/index.ts",
      "default": "./lib/index.js"
    },
    "./*.js": {
      "types": "./src/*.ts",
      "development": "./src/*.ts",
      "default": "./lib/*.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w"
  },
  "files": [
    "lib"
  ],
  "repository": "https://github.com/WhiteMinds/BilibiliRecorder",
  "author": "WhiteMind",
  "license": "LGPL",
  "dependencies": {
    "@autorecord/manager": "workspace:*",
    "blive-message-listener": "^0.5.0",
    "mitt": "^3.0.1"
  }
}
