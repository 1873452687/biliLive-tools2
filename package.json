{
  "name": "biliLive-tools",
  "version": "0.13.0",
  "description": "B站直播录制处理工具",
  "main": "./out/main/index.js",
  "author": "renmu123",
  "license": "GPL-3.0",
  "homepage": "https://github.com/renmu123/biliLive-tools",
  "type": "module",
  "scripts": {
    "dev": "chcp 65001 && electron-vite dev",
    "devMain": "chcp 65001 && electron-vite dev --watch",
    "start": "electron-vite preview",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:win-no-ffmpeg": "npm run build && electron-builder --win --config electron-builder-no-ffmpeg.yml",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "test": "vitest"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@napi-rs/canvas": "^0.1.51",
    "@renmu/bili-api": "^1.3.1",
    "ass-compiler": "^0.1.11",
    "electron-log": "^5.1.2",
    "electron-updater": "^6.1.8",
    "express": "^4.19.2",
    "fast-xml-parser": "^4.3.6",
    "fluent-ffmpeg": "github:renmu123/node-fluent-ffmpeg",
    "fs-extra": "^11.2.0",
    "lodash-es": "^4.17.21",
    "nodemailer": "^6.9.13",
    "ntsuspend": "^1.0.2",
    "pinia": "^2.1.7",
    "semver": "^7.6.0",
    "sortablejs": "^1.15.2",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.2",
    "@electron-toolkit/eslint-config-ts": "^1.0.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@rushstack/eslint-patch": "^1.10.2",
    "@types/express": "^4.17.21",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/node": "20.12.2",
    "@types/nodemailer": "^6.4.14",
    "@vicons/ionicons5": "^0.12.0",
    "@vicons/material": "^0.12.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vueuse/core": "^10.9.0",
    "electron": "^29.3.0",
    "electron-builder": "^24.13.3",
    "electron-devtools-installer": "^3.2.0",
    "electron-vite": "^2.1.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.24.1",
    "less": "^4.2.0",
    "naive-ui": "^2.38.1",
    "prettier": "^3.2.5",
    "qrcode-vue3": "^1.6.8",
    "typescript": "^5.4.5",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.2.8",
    "vitest": "^1.5.0",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "vue-tsc": "^2.0.13"
  }
}
