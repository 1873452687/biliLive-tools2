{
  "extends": "@electron-toolkit/tsconfig/tsconfig.node.json",
  "include": ["electron.vite.config.*", "src/main/**/*", "src/preload/*","src/types/*.d.ts"],
  "compilerOptions": {
    "composite": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "types": ["electron-vite/node"],
    "baseUrl": ".",
    "paths": {
      "@types/*": [
        "src/types/*"
      ]
    }
  }
}
